def reverse(lst):
  # similar to reduce(cons, [], lst)
  def recurse(curr, lst):
    if len(lst) == 0:
      return curr
    else:
      let x = head(lst)
      let curr = cons(x, curr)
      return recurse(curr, tail(lst))
  return recurse([], lst)


check_expect reverse([1, 2, 3]), [3, 2, 1]
check_expect reverse([]), []

