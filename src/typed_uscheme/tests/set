;
; Setting variables.
;

(val test-val 0)
(set test-val 1)

(check-type (set test-val 1) unit)
(check-expect test-val 1)
(check-type-error (set test-val #t))
(check-expect (let ((test-val 2)) (begin (set test-val 3) test-val)) 3)
(check-expect (begin (let ((test-val 2)) (set test-val 3)) test-val) 1)
