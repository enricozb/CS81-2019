;
; Check that the type system does not allow ill-kinded types.
;

(define unit unit (begin))

(check-type-error (@ int))
(check-type-error (@ int unit))
(check-type-error (@ null))
(check-type-error (@ null int int))

(check-type-error (lambda ((x : list)) unit))
(check-type-error (lambda ((x : (int))) unit))
(check-type-error (lambda ((x : (list list))) unit))
(check-type-error (lambda ((x : (int list))) unit))
